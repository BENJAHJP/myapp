                    android:importantForAutofill="no"
                    android:layout_marginEnd="5dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_marginStart="0dp"
                android:padding="5dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_marginStart="7dp"
                    android:id="@+id/fireExtinguisherText"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/fireExtinguisher"
                    />
                <com.google.android.material.checkbox.MaterialCheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/fireExtinguisherCheckbox"
                    />

                <EditText
                    android:id="@+id/fireExtinguisherEditText"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:background="@drawable/text_input"
                    android:hint="@string/remarks"
                    android:padding="10dp"
                    android:inputType="text"
                    android:importantForAutofill="no"
                    android:layout_marginEnd="5dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_marginStart="0dp"
                android:padding="5dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_marginStart="7dp"
                    android:id="@+id/firstAidKitText"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/steeringWheel"
                    />
                <com.google.android.material.checkbox.MaterialCheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/firstAidKitCheckbox"
                    />

                <EditText
                    android:id="@+id/firstAidKitEditText"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:background="@drawable/text_input"
                    android:hint="@string/remarks"
                    android:padding="10dp"
                    android:inputType="text"
                    android:importantForAutofill="no"
                    android:layout_marginEnd="5dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_marginStart="0dp"
                android:padding="5dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_marginStart="7dp"
                    android:id="@+id/towingRopeText"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/towingRope"
                    />
                <com.google.android.material.checkbox.MaterialCheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/towingRopeCheckbox"
                    />

                <EditText
                    android:id="@+id/towingRopeEditText"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:background="@drawable/text_input"
                    android:hint="@string/remarks"
                    android:padding="10dp"
                    android:inputType="text"
                    android:importantForAutofill="no"
                    android:layout_marginEnd="5dp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_marginStart="0dp"
                android:padding="5dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_marginStart="7dp"
                    android:id="@+id/ownersManualText"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/ownersManual"
                    />
                <com.google.android.material.checkbox.MaterialCheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ownersManualCheckbox"
                    />

                <EditText
                    android:id="@+id/ownersManualEditText"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="2dp"
                    android:background="@drawable/text_input"
                    android:hint="@string/remarks"
                    android:padding="10dp"
                    android:inputType="text"
                    android:importantForAutofill="no"
                    android:layout_marginEnd="5dp"
                    />

            </LinearLayout>

            <EditText
                android:layout_width="350dp"
                android:layout_height="80dp"
                android:background="@drawable/text_input"
                android:hint="@string/general"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                />
        </LinearLayout>
    </ScrollView>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton7"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="600dp"
        android:elevation="20dp"
        android:src="@drawable/ic_arrow"
        app:fabCustomSize="70dp"
        app:fabSize="normal"
        android:backgroundTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>